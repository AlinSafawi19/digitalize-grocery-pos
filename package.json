{
  "name": "digitalize-grocery-pos",
  "version": "1.0.1",
  "description": "DigitalizePOS - Professional Desktop Point of Sale System",
  "main": "dist-electron/main.js",
  "author": "DigitalizePOS",
  "license": "PROPRIETARY",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:main": "tsc -p tsconfig.node.json",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:clean": "node scripts/cleanup-build.js",
    "electron:build": "node scripts/cleanup-build.js && prisma generate && vite build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "cross-env DATABASE_URL=\"file:./prisma/database/digitalizePOS.db\" prisma migrate dev",
    "prisma:reset": "cross-env DATABASE_URL=\"file:./prisma/database/digitalizePOS.db\" prisma migrate reset",
    "prisma:studio": "cross-env DATABASE_URL=\"file:./prisma/database/digitalizePOS.db\" prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "generate:secret": "node scripts/generate-secret.js",
    "generate:icons": "node scripts/generate-icons.js",
    "update:generate-yml": "node scripts/generate-latest-yml.js",
    "update:upload": "node scripts/upload-update-files.js",
    "update:verify": "node scripts/verify-update-server.js",
    "update:verify-match": "node scripts/verify-file-match.js",
    "version:patch": "node scripts/bump-version.js patch",
    "version:minor": "node scripts/bump-version.js minor",
    "version:major": "node scripts/bump-version.js major",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^8.19.0",
    "@mui/x-date-pickers-pro": "^8.19.0",
    "@prisma/client": "^5.7.0",
    "@reduxjs/toolkit": "^2.0.0",
    "@types/jquery": "^3.5.33",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.3",
    "@types/react-window": "^1.8.8",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.0",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.6.2",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.1.1",
    "jquery": "^3.7.1",
    "jspdf": "^2.5.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "node-cron": "^4.2.1",
    "node-machine-id": "^1.1.12",
    "pdf-to-printer": "^5.6.1",
    "pdfkit": "^0.17.2",
    "prisma": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-phone-number-input": "^3.4.14",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.20.0",
    "react-window": "^2.2.3",
    "recharts": "^2.10.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/plist": "^3.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "electron": "^27.0.0",
    "electron-builder": "^24.9.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "to-ico": "^1.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.digitalize.pos",
    "productName": "DigitalizePOS",
    "directories": {
      "output": "release"
    },
    "asar": false,
    "compression": "normal",
    "afterPack": "./scripts/copy-prisma-after-pack.js",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "!node_modules/.cache/**/*",
      "!node_modules/**/test/**/*",
      "!node_modules/**/tests/**/*",
      "!node_modules/**/*.test.js",
      "!node_modules/**/*.spec.js",
      "package.json",
      "prisma/schema.prisma",
      "prisma/migrations/**"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "sign": null,
      "signingHashAlgorithms": [],
      "certificateFile": null,
      "certificatePassword": null
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "differentialPackage": false,
      "artifactName": "grocery-pos-${version}.${ext}",
      "allowToChangeInstallationDirectory": false,
      "allowElevation": true,
      "runAfterFinish": false,
      "warningsAsErrors": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "include": "build/nsis-close-app.nsh"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://downloads.digitalizepos.com"
      }
    ]
  }
}
